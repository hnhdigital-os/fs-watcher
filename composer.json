{
    "name": "hnhdigital-os/fs-watcher",
    "description": "Provides the ability to watch folders for changes and call a binary in response.",
    "keywords": ["php", "inotify", "filesystem", "watcher", "cli"],
    "homepage": "https://github.com/hnhdigital-os",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hnhdigital-os/fs-watcher/issues",
        "source": "https://github.com/hnhdigital-os/fs-watcher"
    },
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@hnh.digital"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-inotify": "*",
        "hnhdigital-os/php-cli-helper": "^1.1",
        "laminas/laminas-text": "^2.8",
        "padraic/phar-updater": "^1.0.6",
        "symfony/yaml": "^5.2",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "laravel-zero/framework": "^8.4",
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["build/fs-tasker"]
}
